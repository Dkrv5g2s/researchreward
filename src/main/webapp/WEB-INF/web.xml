<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>servlet.TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DistinguishedProfessorFormServlet</servlet-name>
        <servlet-class>servlet.DistinguishedProfessor.DistinguishedProfessorFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DistinguishedProfessorFormServlet</servlet-name>
        <url-pattern>/DistinguishedProfessorForm</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ApplicationDocInstructionsServlet</servlet-name>
        <servlet-class>servlet.DistinguishedProfessor.ApplicationDocInstructionsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApplicationDocInstructionsServlet</servlet-name>
        <url-pattern>/ApplicationDocInstructions</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DistinguishedProfessorTableAServlet</servlet-name>
        <servlet-class>servlet.DistinguishedProfessor.DistinguishedProfessorTableAServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DistinguishedProfessorTableAServlet</servlet-name>
        <url-pattern>/DistinguishedProfessorTableA</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DistinguishedProfessorTableBServlet</servlet-name>
        <servlet-class>servlet.DistinguishedProfessor.DistinguishedProfessorTableBServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DistinguishedProfessorTableBServlet</servlet-name>
        <url-pattern>/DistinguishedProfessorTableB</url-pattern>
    </servlet-mapping>
</web-app>